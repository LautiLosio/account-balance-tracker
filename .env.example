# Auth0
AUTH0_SECRET=replace-with-a-long-random-string
# Either APP_BASE_URL or AUTH0_BASE_URL
APP_BASE_URL=http://localhost:3000
AUTH0_BASE_URL=http://localhost:3000
# Either AUTH0_DOMAIN or AUTH0_ISSUER_BASE_URL
AUTH0_DOMAIN=YOUR_TENANT_REGION.auth0.com
AUTH0_ISSUER_BASE_URL=https://YOUR_TENANT_REGION.auth0.com
AUTH0_CLIENT_ID=your-auth0-client-id
AUTH0_CLIENT_SECRET=your-auth0-client-secret

# Vercel KV (recommended connection string)
KV_URL=redis://default:password@host:port

# Vercel KV REST API (required when KV_URL is not set)
KV_REST_API_URL=https://example-kv.upstash.io
KV_REST_API_TOKEN=your-kv-rest-api-token
# Optional read-only token
KV_REST_API_READ_ONLY_TOKEN=your-kv-rest-api-read-only-token
